<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="entities.Filiere" table="filiere" schema="fpl2023">
        <id name="id">
            <column name="ID" sql-type="int(25)"/>
        </id>
        <property name="code">
            <column name="CODE" sql-type="varchar(255)"/>
        </property>
        <property name="libelle">
            <column name="LIBELLE" sql-type="varchar(255)"/>
        </property>
        <set name="etudiants" inverse="true">
            <key>
                <column name="FILIERE" not-null="true"/>
            </key>
            <one-to-many not-found="ignore" class="entities.Etudiant"/>
        </set>
    </class>
</hibernate-mapping>